<% data.timelines.common.forEach(function(timeline, idx) { %>
    <%==
        data._t.timeline({
            timeline : timeline,
            path     : data.path
        })
    %>
<% }) %>

<% data.timelines.lists.forEach(function(timeline, idx) { %>
    <%==
        data._t.timeline({
            timeline : timeline,
            path     : data.path,
            divider  : (idx === 0)
        })
    %>
<% }) %>

<% data.timelines.searches.forEach(function(timeline, idx) { %>
    <%==
        data._t.timeline({
            timeline : timeline,
            path     : data.path,
            divider  : (idx === 0)
        })
    %>
<% }) %>
